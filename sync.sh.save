#!/usr/bin/env bash
# force_sync.sh â€” force-push ~/sow_clean to GitHub

set -e

echo "Removing old remote (if any)..."
git remote remove origin 2>/dev/null || true

echo "Adding remote origin..."
git remote add origin git@github.com:DurangoDavid/SOW.git

echo "Ensuring main branch..."
git branch -M main

echo "Staging all files..."
git add .

echo "Committing changes..."
git commit -m "Force sync: clean SOW project" --allow-empty

echo "Force-pushing to origin/main..."
git push --force origin main

echo "Sync complete."





















